<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@+id/navigation_data_station">
    <fragment
        android:id="@+id/navigation_data_station"
        android:name="com.careful.HyperFVM.Fragments.DataStation.DataStationFragment"
        android:label="@string/label_data_station"
        tools:layout="@layout/fragment_data_station" />

    <fragment
        android:id="@+id/navigation_tools"
        android:name="com.careful.HyperFVM.Fragments.Tools.ToolsFragment"
        android:label="@string/label_tools"
        tools:layout="@layout/fragment_tools" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.careful.HyperFVM.Fragments.Settings.SettingsFragment"
        android:label="@string/label_settings"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_about_app"
        android:name="com.careful.HyperFVM.Fragments.AboutApp.AboutAppFragment"
        android:label="@string/label_about_app"
        tools:layout="@layout/fragment_about_app" />
