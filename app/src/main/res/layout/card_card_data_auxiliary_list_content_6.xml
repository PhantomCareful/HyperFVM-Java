<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="25dp"
    app:cardBackgroundColor="?attr/GeneralCardViewBackground"
    app:cardCornerRadius="20dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/card_card_data_index_3_1_1"
            layout="@layout/card_card_data_index_3_1_1" />

        <include
            android:id="@+id/card_card_data_index_3_1_2"
            layout="@layout/card_card_data_index_3_1_2" />

        <include
            android:id="@+id/card_card_data_index_3_1_3"
            layout="@layout/card_card_data_index_3_1_3" />

        <include
            android:id="@+id/card_card_data_index_3_1_4"
            layout="@layout/card_card_data_index_3_1_4" />

        <include
            android:id="@+id/card_card_data_index_3_1_5"
            layout="@layout/card_card_data_index_3_1_5" />

        <include
            android:id="@+id/card_card_data_index_3_2_2"
            layout="@layout/card_card_data_index_3_2_2" />

        <include
            android:id="@+id/card_card_data_index_3_2_3"
            layout="@layout/card_card_data_index_3_2_3" />

        <include
            android:id="@+id/card_card_data_index_3_2_4"
            layout="@layout/card_card_data_index_3_2_4" />

        <TextView
            android:id="@+id/title_card_data_auxiliary_list_6_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:text="@string/title_card_data_auxiliary_list_6_1"
            android:textColor="?attr/colorOnPrimaryContainer"
            android:textSize="20sp"
            android:textStyle="bold"
            android:gravity="center"
            android:clickable="true"
            android:background="?attr/selectableItemBackground"/>

        <include
            android:id="@+id/card_card_data_index_3_2_5_1"
            layout="@layout/card_card_data_index_3_2_5" />

        <include
            android:id="@+id/card_card_data_index_16_1_3_1"
            layout="@layout/card_card_data_index_16_1_3" />

        <TextView
            android:id="@+id/title_card_data_auxiliary_list_6_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:text="@string/title_card_data_auxiliary_list_6_2"
            android:textColor="?attr/colorOnPrimaryContainer"
            android:textSize="20sp"
            android:textStyle="bold"
            android:gravity="center"
            android:clickable="true"
            android:background="?attr/selectableItemBackground"/>

        <include
            android:id="@+id/card_card_data_index_3_2_1"
            layout="@layout/card_card_data_index_3_2_1" />

        <include
            android:id="@+id/card_card_data_index_11_3_7"
            layout="@layout/card_card_data_index_11_3_7" />

        <include
            android:id="@+id/card_card_data_index_3_2_5_2"
            layout="@layout/card_card_data_index_3_2_5" />

        <include
            android:id="@+id/card_card_data_index_16_1_3_2"
            layout="@layout/card_card_data_index_16_1_3" />

        <include
            android:id="@+id/card_card_data_index_3_2_6"
            layout="@layout/card_card_data_index_3_2_6" />

        <include
            android:id="@+id/card_card_data_index_3_2_7"
            layout="@layout/card_card_data_index_3_2_7" />

        <include
            android:id="@+id/card_card_data_index_1_1_7"
            layout="@layout/card_card_data_index_1_1_7" />

        <include
            android:id="@+id/card_card_data_index_1_1_8"
            layout="@layout/card_card_data_index_1_1_8" />

    </LinearLayout>

</androidx.cardview.widget.CardView>