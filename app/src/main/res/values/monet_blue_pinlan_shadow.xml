<resources>
    <style name="Monet.Blue.Pinlan.Shadow" parent="Theme.Material3.Light.NoActionBar">
        <!--Button的默认样式-->
        <item name="materialButtonStyle">@style/Button.Shadow</item>

        <!--CardView的默认样式-->
        <item name="cardViewStyle">@style/CardView.Shadow</item>
        <!--卡片下间距-->
        <item name="CardViewMarginBottom">15dp</item>

        <!--一般CardView-->
        <!--卡片颜色-->
        <item name="GeneralCardViewBackground">?attr/colorPrimaryContainer</item>
        <!--文字颜色-->
        <item name="TextViewColorOnGeneralCardView">?attr/colorOnPrimaryContainer</item>
        <!--矢量图颜色-->
        <item name="VectorGraphicColorOnGeneralCardView">?attr/colorOnPrimaryContainer</item>

        <!--重要CardView-->
        <!--卡片颜色-->
        <item name="ImportantCardViewBackground">?attr/colorPrimary</item>
        <!--文字颜色-->
        <item name="TextViewColorOnImportantCardView">?attr/colorOnPrimary</item>
        <!--矢量图颜色-->
        <item name="VectorGraphicColorOnImportantCardView">?attr/colorOnPrimary</item>

        <item name="colorPrimary">@color/md_blue_pinlan_primary</item>
        <item name="colorOnPrimary">@color/md_blue_pinlan_onPrimary</item>
        <item name="colorPrimaryContainer">@color/md_blue_pinlan_primaryContainer</item>
        <item name="colorOnPrimaryContainer">@color/md_blue_pinlan_onPrimaryContainer</item>
