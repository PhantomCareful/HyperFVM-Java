        <item name="GeneralCardViewBackground">?attr/colorSurfaceContainer</item>
        <!--文字颜色-->
        <item name="TextViewColorOnGeneralCardView">?attr/colorPrimary</item>
        <!--矢量图颜色-->
        <item name="VectorGraphicColorOnGeneralCardView">?attr/colorPrimary</item>

        <!--重要CardView-->
        <!--卡片颜色-->
        <item name="ImportantCardViewBackground">?attr/colorTertiaryContainer</item>
        <!--文字颜色-->
        <item name="TextViewColorOnImportantCardView">?attr/colorOnTertiaryContainer</item>
        <!--矢量图颜色-->
        <item name="VectorGraphicColorOnImportantCardView">?attr/colorOnTertiaryContainer</item>

        <item name="colorPrimary">@color/md_purple_ningyezi_primary</item>
